{
  "name": "js-selenium-boilerplate",
  "version": "1.0.0",
  "description": "Example to start an automation project using javascript and selenium",
  "main": "config.js",
  "scripts": {
    "test": "node node_modules/mocha/bin/mocha ./__tests__/**/*.ui.js --timeout=60000",
    "test-watch": "node node_modules/mocha/bin/mocha ./__tests__/**/*.ui.js --watch --recursive --timeout=60000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FernandoZnga/js-selenium-boilerplate.git"
  },
