  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FernandoZnga/js-selenium-boilerplate/issues"
  },
  "homepage": "https://github.com/FernandoZnga/js-selenium-boilerplate/blob/master/README.md",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^6.1.0",
    "mocha": "^5.2.0",
    "selenium-webdriver": "4.0.0-alpha.1"
  }
}
